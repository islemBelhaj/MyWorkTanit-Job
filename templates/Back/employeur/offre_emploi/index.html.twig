

{% extends 'base.html.twig' %}

{% block title %}OffreEmploi index{% endblock %}

{% block body %}
    <h1>OffreEmploi index</h1>

    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>Titre</th>
            <th>NbrePostVacant</th>
            <th>RemunirationPropose</th>
            <th>DescriptionEmploi</th>
            <th>Experience</th>

            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for offre_emploi in offre_emplois %}
            <tr>
                <td>{{ offre_emploi.id }}</td>
                <td>{{ offre_emploi.titre }}</td>
                <td>{{ offre_emploi.nbrePostVacant }}</td>
